{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "es2016", "es2017", "dom"],
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "dist",
    "removeComments": true,
    "strict": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "baseUrl": "./src",
    "paths": {
      "src/*": ["."]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"]
}

